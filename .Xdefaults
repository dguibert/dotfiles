*loginShell:                   true

URxvt.searchable-scrollback:   CM-s
URxvt.utf8:                    true

URxvt.transparent:             false
URxvt.depth: 32
URxvt.intensityStyles: false
URxvt.termName:                xterm-256color

!Emacs.FontBackend: xft
